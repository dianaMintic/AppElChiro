<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".datosrden">

    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bottomAppBar4"
        style="@style/Widget.MaterialComponents.BottomAppBar.Colored"
        android:layout_width="415dp"
        android:layout_height="65dp"
        android:layout_gravity="bottom"
        app:backgroundTint="#DAD7D7"
        app:contentInsetStart="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="2dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="55dp"
        android:layout_height="62dp"
        app:srcCompat="@drawable/home2"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="671dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/carro2"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="671dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/buscar2"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="667dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_marginBottom="336dp"
        android:fontFamily="serif"
        android:gravity="left"
        android:text="OC - ####"
        android:textColor="#0C0C0C"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.074"
        tools:ignore="ExtraText" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="390dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="416dp"
        android:autoText="true"
        android:gravity="center"
        android:text="Se ha generado de forma exitosa tu pedido"
        android:textColor="#1E1D1D"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_marginBaseline="10sp"
        tools:layout_editor_absoluteX="16dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="135dp"
        android:layout_height="144dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@drawable/logocopia" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_marginBottom="500dp"
        android:fontFamily="serif"
        android:gravity="left"
        android:text="Datos de Compra"
        android:textColor="#0C0C0C"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.074"
        tools:ignore="ExtraText" />
</androidx.constraintlayout.widget.ConstraintLayout>